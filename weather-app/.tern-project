{
  "ecmaVersion": 6,
  "libs": [
    "browser",
    "jquery"
  ],
  "loadEagerly": [
    "src/**/*.js",
    "test/**/*.js"
  ],
  "dontLoad": [
    "node_modules/**/*.js"
  ],
  "plugins": {
    "complete_strings": {
      "maxLength": 15
    },
    "node": {},
    "lint": {},
    "doc_comment": {
      "fullDocs": true,
      "strong": true
    }
  }
}
